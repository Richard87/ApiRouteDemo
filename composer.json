{
    "autoload": {
        "psr-4": {
            "App\\": "src",
            "Tests\\": "tests"
        }
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "symfony/web-profiler-bundle": "^5.2",
        "symfony/var-dumper": "^5.2",
        "roave/security-advisories": "dev-master",
        "symfony/debug-bundle": "^5.2",
        "phpunit/phpunit": "^9.5",
        "symfony/flex": "^1.11",
        "symfony/console": "^5.2",
        "symfony/monolog-bundle": "^3.0",
        "symfony/stopwatch": "^5.2",
        "symfony/dotenv": "^5.2",
        "symfony/yaml": "^5.2",
        "composer/package-versions-deprecated": "1.11.99.1",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/orm": "^2.8",
        "symfony/proxy-manager-bridge": "^5.2",
        "symfony/apache-pack": "^1.0"
    },
    "require": {
        "php": "^8.0",
        "doctrine/inflector": "^2.0",
        "symfony/routing": "^5.0",
        "richard87/api-route": "dev-master"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../ApiRoute"
        }
    ],
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        }
    },
    "name": "richard87/api_route",
    "description": "ApiRoute and Bundle for Symfony. Introspects your code and generates fully typed OpenAPI Specifications, Swagger UI and code generation for frontend libraries!"
}
